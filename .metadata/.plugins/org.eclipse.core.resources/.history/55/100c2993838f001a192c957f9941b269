package github.domain;

import github.service.Status;

public class Architect extends Designer{
	private int stock;	//表示公司奖励的股票数量

	public Architect() {
		super();
	}
	
	public Architect(int id, String name, int age, double salary, int memeberld, Status status, Equipment equipment,
			double bonus, int stock) {
		super(id, name, age, salary, memeberld, status, equipment, bonus);
		this.stock = stock;
	}

	public int getStock() {
		return stock;
	}

	public void setStock(int stock) {
		this.stock = stock;
	}
	
}
