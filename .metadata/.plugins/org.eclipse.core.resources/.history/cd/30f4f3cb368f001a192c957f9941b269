package github3;

public class StudentTest {
	public static void main(String[] args) {
		Student s = new Student();
		s.regist(1001);
		System.out.println(s);
	}
}
class Student{
	private int id;
	
	public void regist(int id){
		if(id > 0){
			this.id = id;
		}else{
			System.out.println("您输入的数据非法！");
		}
	}

	public Student(int id) {
		super();
		this.id = id;
	}
	
}