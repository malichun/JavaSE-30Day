package git3;
/*
 * 定义一个Circle类，声明redius属性，提供getter和setter方法
 */
public class Circle {
	private Double redius;

	public Double getRedius() {
		return redius;
	}

	public void setRedius(Double redius) {
		this.redius = redius;
	}

	public Circle() {
		super();
	}

	public Circle(Double redius) {
		super();
		this.redius = redius;
	}

}
